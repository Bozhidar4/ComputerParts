<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Computer Parts</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")


    <link href="~/Content/flexslider.css" rel="stylesheet" />
    <link href="~/Content/form.css" rel="stylesheet" />
    <link href="~/Content/style.css" rel="stylesheet" />
    <link href="~/Content/jquery-ui.css" rel="stylesheet" />
    <link href="~/Content/memenu.css" rel="stylesheet" />

    <!-- Custom CSS -->
    <link href="css/shop-homepage.css" rel="stylesheet">
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/shop-homepage.css" rel="stylesheet" />
    <link href="~/Content/Site.css" rel="stylesheet" />
</head>
<body>
    <!-- Navigation -->
    <div class="header-top">
        <div class="header-bottom">
            <div class="logo">
                <h1><a href="#">Computer Parts</a></h1>
            </div>
            <!---->
            <div class="top-nav">
                <ul class="memenu skyblue">
                    <li>@Html.ActionLink("Home", "Index", "Home")</li>
                    <li>@Html.ActionLink("About", "About", "Home")</li>
                    <li>@Html.ActionLink("Get Started", "Index", "Cases")</li>
                    <li>@Html.ActionLink("Contact", "Contact", "Home")</li>
                </ul>
            </div>
            <!---->
            <div class="cart box_1">
                <div class="clearfix"> </div>
            </div>
            <div class="clearfix"> </div>
            <!---->
        </div>
        <div class="clearfix"> </div>
    </div>

    <div class="container body-content">
        @RenderBody()
        <hr />
        <!--start-image-cursuals-->
        <div id="scroller-wrapper">
            <div id="scroller">
                <div>
                    <img class="logos" src="https://upload.wikimedia.org/wikipedia/commons/thumb/9/93/Intel_Logo.svg/2000px-Intel_Logo.svg.png">
                    <img class="logos" src="http://3.bp.blogspot.com/-qF4UqIhxrXM/VW1oFuKTLOI/AAAAAAAAC3E/G-GvBmijUBg/s1600/AMD-Vector-logo.png">
                    <img class="logos" src="https://upload.wikimedia.org/wikipedia/commons/thumb/d/d5/Gigabyte_Technology_Logo.svg/2000px-Gigabyte_Technology_Logo.svg.png">
                    <img class="logos" src="https://www.pcper.com/files/imagecache/article_max_width/news/2012-07-05/asus_logo.png">
                    <img class="logos" src="https://cdn2.hubspot.net/hubfs/472663/nvidia_logo_horizontal.png">
                    <img class="logos" src="http://s7d2.scene7.com/is/image/SamsungUS/samsung-logo-191-1">
                    <img class="logos" src="http://cdn.wccftech.com/wp-content/uploads/2015/05/AMD-Radeon-Logo.png">
                </div>
            </div>
        </div>
       


        
    </div>
    <footer>
        <p>&copy; @DateTime.Now.Year - by Bozhidar Rangelov</p>
    </footer>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)



    <script src="~/js/simpleCart.min.js"></script>
    <script src="~/js/responsiveslides.min.js"></script>
    <script src="~/js/imagezoom.js"></script>
    <script src="~/js/jquery-ui.min.js"></script>
    <script src="~/js/jquery.flexisel.js"></script>
    <script src="~/js/jquery.flexslider.js"></script>
    <script src="~/js/jquery.jscrollpane.min.js"></script>
    <script src="~/js/memenu.js"></script>
    <script src="~/js/popper.js"></script>
    <script src="~/Scripts/bootstrap.js"></script>
    <script src="~/js/logos-slider.js"></script>
    <script src="~/js/simpleCart.min.js"></script>
</body>
</html>
